/* random/random.wxss */
page{
  height:100%;
}
.root {
  width: 100%;
   height: 100%;
   background-color: #70c7da;
   position: relative;
}
.background-image{
  height : 140%;
   position: absolute;
   width: 100%;
   left: 0px;
   top: 0px;
}
.content{
   position: absolute;
   width: 100%;
   height: 100%;
   left: 0px;
   top: 0px;
}
.butt{
  position: fixed; 
  left: 35%  ;
  top : 65%; 
  width: 100%; 
  height: 70rpx; 
  display: flex; 
  box-sizing: border-box
}
.text1{
 
  left: 35%  ;
  top : 70%; 
  width: 100%; 
  height: 70rpx; 
  display: flex; 
  font-size: 40pX;
  box-sizing: border-box
}

.text{
  display: inline-block;
  font-size: 70pX;
  padding: 2pX;
}
.mask{
  position: absolute;
  width: 100%;
  height: 100%;
  left: 0px;
  top: 0px;
  background-color:rgba(0, 0, 0, .2);
  z-index: 2;
}
.eject{
  display: flex;
  position: fixed;
  width: 80%;
  height: 50%;
  left: 10%;
  top: 25%;
  background-color:rgba(255, 255, 255, 0.911);
  justify-content:center;
  align-items: center;
  z-index: 3;
  margin: 1%;
  border-radius: 3%;
}
.table{
 
  width: 98%;

  border-top: #E8E8E8 solid 1px;

  border-bottom: #E8E8E8 solid 1px;

  position: relative;

  left: 1%;

  color: #333333;

}

.tab_tr{

  width: 100%;

  position: relative;

  height: 50rpx;

  border-top: #E8E8E8 solid 1px;

  border-bottom: #e8e8e83d solid 1px;

  background-color: #ffffffa6;

  display: flex;

  align-items: center;

  justify-content: center;

}

.tr_td1{

  width: 15%;

  height: 50rpx;

  border-top: #E8E8E8 solid 1px;

  border-bottom: #E8E8E8 solid 1px;

  background-color: #ffffffa6;

  display: flex;

  align-items: center;

  justify-content: center;

}

.tr_td2{

  width: 40%;

  height: 50rpx;

  border: 1px solid #E8E8E8;

  background-color:#ffffffa6;

  display: flex;

  align-items: center;
  
  justify-content: center;

}

.tr_td3{

  width: 50%;

  height: 50rpx;

  border: 1px solid#E8E8E8;

  background-color: #ffffffa6;

  display: flex;

  align-items: center;

  justify-content: center;

}



.text3{

  font-size:10px;

  font-family:PingFangHK-Regular,PingFangHK;
  
  font-weight:400;

  color:rgb(0, 0, 0);

  line-height:17px;

}